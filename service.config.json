		"network_admin":2,
		"base_user":3,
		"data_governer":4,
		"guest_user":5
	},
	"permissions": {
		"oauth":{},
		"system":{
			"networks.read":2,
			"networks.write":3,
			"root":10,
			"permissions.read":11,
			"permissions.write":12,
			"apps.read":13,
			"apps.write":14
		},
		"dashboard":{
			"base.access":4,
			"users.permissions.read":5,
			"users.permissions.write":6,
			"users.permissions.list":7,
			"users.list":8,
